{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "commonjs",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "./*"
      ]
      
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.js", 
    "**/*.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]


}

